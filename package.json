    "@mui/styles": "*",
    "codemirror-github-dark": "*",
    "codemirror-github-light": "*",
    "framer-motion": "*",
    "graphiql": "*",
    "graphql": "*",
    "graphql-request": "*",
    "graphql-ws": "*",
    "next": "^13",
    "react": "^18",
    "react-dom": "^18",
    "react-is": "^18",
    "react-no-ssr": "*",
    "web-vitals": "*"
  },
  "devDependencies": {
    "@types/node": "*",
    "@types/react": "^18",
    "@types/react-no-ssr": "*",
    "autoprefixer": "*",
    "postcss": "*",
