{
  "name": "graphql-client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build && next export",
    "export": "echo \"no action in script. Please run yarn build (includes next export)\"",
    "start": "next start",
    "analyze": "ANALYZE=true next build",
    "format-check": "rome format src",
    "format": "rome format --write src",
    "check": "rome check src",
    "check-fix": "rome check --apply src",
    "lint": "yarn format-check && yarn check",
    "lint-fix": "yarn format && yarn check-fix",
    "prelint": "yarn install",
    "prebuild": "yarn install",
    "predev": "yarn install",
    "preanalyze": "yarn install",
    "all": "yarn lint && yarn build"
  },
  "dependencies": {
    "@emotion/react": "*",
    "@emotion/server": "*",
    "@emotion/styled": "*",
